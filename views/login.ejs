<form action="/login" method="post">
  <input type="hidden" name="_csrf" value="<%= csrfToken %>">
  <label>
    아이디
    <input type="text" name="username">
  </label>
  <label>
    암호
    <input type="password" name="password">
  </label>
  <input type="submit" value="로그인">
</form>

<% errors.forEach(error => { %>
  <p><%= error %></p>
<% }) %>